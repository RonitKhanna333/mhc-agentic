# LLM Provider
LLM_PROVIDER=groq

# Groq Configuration
GROQ_API_KEY=your_groq_api_key_here
GROQ_MODEL=llama-3.1-8b-instant
GROQ_MASTER_MODEL=llama-3.3-70b-versatile

# Gemini Configuration (alternative)
# GEMINI_API_KEY=your_gemini_api_key_here
# GEMINI_MODEL=gemini-1.5-flash
# GEMINI_MASTER_MODEL=gemini-1.5-pro

# Agent-Lightning Tracing
# Enable trace collection for training and optimization
# Default: false (no performance impact when disabled)
ENABLE_TRACING=false

# Debug Mode
# Shows internal processing steps
DEBUG_MODE=false
